%h1 Listing cats
%table
  %tr
    %th Name
    %th Age
    %th Description
    %th Image
    %th
    %th
    %th
  - @cats.each do |cat|
    %tr
      %td= cat.name
      %td= cat.age
      %td= cat.description
      %td= cat.image
      %td= link_to 'Show', cat
      %td= link_to 'Edit', edit_cat_path(cat)
      %td= link_to 'Destroy', cat, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Cat', new_cat_path